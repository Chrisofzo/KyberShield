{
  "name": "kybershield-vpn",
  "version": "1.0.0",
  "description": "KyberShield VPN - Quantum-Resistant Security for Your Digital Life",
  "main": "electron.js",
  "homepage": "./",
  "author": "KyberShield Technologies",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-build": "electron-builder",
    "dist": "npm run build && electron-builder --publish never",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.kybershield.vpn",
    "productName": "KyberShield VPN",
    "files": [
      "build/**/*",
      "electron.js",
      "assets/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "artifactName": "KyberShieldVPN-${version}-${arch}.dmg",
    "asar": true,
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities",
      "icon": "assets/icon.png"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^5.1.0"
  }
}
