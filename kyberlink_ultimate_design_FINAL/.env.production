# Production environment variables for kybershield.io
FLASK_ENV=production
FLASK_SECRET_KEY=generate-a-secure-random-key-here
DOMAIN=kybershield.io

# VPN Server Configuration
VPN_SERVER_US_EAST=vpn-us-east.kybershield.io
VPN_SERVER_US_WEST=vpn-us-west.kybershield.io
VPN_SERVER_EU=vpn-eu.kybershield.io
VPN_SERVER_ASIA=vpn-asia.kybershield.io

# Security Settings
ENABLE_HTTPS_REDIRECT=true
ENABLE_RATE_LIMITING=true
MAX_REQUESTS_PER_MINUTE=60

# Session Configuration
SESSION_LIFETIME_DAYS=7
SECURE_COOKIES=true

# Future integrations (optional)
# DATABASE_URL=postgresql://...
# REDIS_URL=redis://...
# MAIL_SERVER=smtp.kybershield.io
# MAIL_USERNAME=noreply@kybershield.io