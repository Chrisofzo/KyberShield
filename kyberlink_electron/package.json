{
  "name": "kybershield-vpn",
  "version": "1.0.2",
  "description": "Quantum-resistant VPN",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.kybershield.vpn",
    "productName": "KyberShield VPN",
